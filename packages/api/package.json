{
  "name": "@settler/api",
  "version": "1.0.0",
  "description": "Settler API Server - Serverless reconciliation service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild": "npm run typecheck",
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "migrate": "tsx src/db/migrate.ts",
    "migrate:prod": "node dist/db/migrate.js",
    "migrate:check": "tsx src/db/migrate.ts --check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@settler/adapters": "*",
    "@settler/types": "*",
    "@settler/sdk": "*",
    "@settler/edge-ai-core": "*",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "rate-limit-redis": "^4.3.1",
    "jsonwebtoken": "^9.0.3",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "pg": "^8.16.3",
    "bcrypt": "^5.1.1",
    "winston": "^3.18.3",
    "uuid": "^9.0.1",
    "async-mutex": "^0.4.0",
    "ioredis": "^5.8.2",
    "bullmq": "^5.65.1",
    "p-retry": "^6.2.0",
    "opossum": "^9.0.0",
    "prom-client": "^15.1.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.52.0",
    "@opentelemetry/auto-instrumentations-node": "^0.52.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.52.0",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "envalid": "^8.1.1",
    "isomorphic-dompurify": "^2.33.0",
    "@sentry/node": "^7.91.0",
    "@sentry/profiling-node": "^7.91.0",
    "@supabase/supabase-js": "^2.86.2",
    "@upstash/redis": "^1.35.7",
    "resend": "^3.2.0",
    "xml2js": "^0.6.2",
    "socket.io": "^4.8.1",
    "exceljs": "^4.4.0",
    "pdfkit": "^0.14.0",
    "csv-writer": "^1.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.11",
    "@types/pg": "^8.15.6",
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/xml2js": "^0.4.14",
    "@types/pdfkit": "^0.13.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^8.57.1",
    "ts-jest": "^29.4.6",
    "jest": "^29.7.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "supertest": "^7.1.4",
    "artillery": "^2.0.27",
    "k6": "^0.0.0"
  }
}
